					qtn = document.getElementById('qt_'+texto[i]).value;
					
					if(qte<qt){
						alert("A quantidade a pagar do produto "+cod+" nÃ£o pode ser superior a quantidade recebida!");
						return false;
						
					}else if((qtn!="")||(nt!="")||(dt!="")){
						if(qtn==""){
							alert("A quantidade do produto "+cod+" nÃ£o pode ficar em branco!");
